# NetSuite OAuth 2.0 Credentials (Required)
# OAuth 2.0 is required as of February 2025 (TBA deprecated)
# From Integration Record (Setup > Integration > Manage Integrations)
NS_CLIENT_ID=your_consumer_key_here
NS_CLIENT_SECRET=your_consumer_secret_here

# PostgreSQL Database Credentials (Required)
DB_USER=postgres
DB_PASSWORD=your_database_password_here

# Salesforce Integration (Optional - for revenue pipeline analysis)
# Set SF_ENABLED=true to enable Salesforce opportunity and account sync
SF_ENABLED=false
SF_INSTANCE_URL=https://your-instance.my.salesforce.com
SF_CLIENT_ID=your_salesforce_consumer_key
SF_CLIENT_SECRET=your_salesforce_consumer_secret
